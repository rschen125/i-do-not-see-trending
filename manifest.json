{
	"manifest_version": 2,
	"name": "i do not see trending",
	"version": "0.1",

	"content_scripts": [
		{
			"matches": [
				"https://*.twitter.com/*"
			],
			"js": [
				"scripts/jquery-3.4.1.min.js",
				"scripts/i_do_not_see_trending.js"
			]
		}
	],
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	}
}
